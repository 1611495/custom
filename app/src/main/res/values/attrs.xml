<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CircleImageView">
        <attr name="circle_width" format="dimension" />
        <attr name="circle_color" format="color" />
        <attr name="android:src" />
    </declare-styleable>

    <declare-styleable name="CalendarView">
        <attr name="cell_width" format="dimension" />
        <attr name="cell_height" format="dimension" />
        <attr name="day_cell_height" format="dimension" />
        <attr name="text_color" format="color" />
        <attr name="select_color" format="color" />
        <attr name="today_color" format="color" />
    </declare-styleable>

    <declare-styleable name="FlowLayout">
        <attr name="vertical_margin" format="dimension" />
        <attr name="horizontal_margin" format="dimension" />
    </declare-styleable>
</resources>